<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>library</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <Platforms>AnyCPU;x64</Platforms>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Jarred Capellman</Authors>
    <Company>Jarred Capellman</Company>
    <Product>WET</Product>
    <LangVersion>9.0</LangVersion>
    <AssemblyVersion>0.5.3.0</AssemblyVersion>
    <FileVersion>0.5.3.0</FileVersion>
    <Version>0.5.3</Version>
    <PackageTags>windows event tracing, process hooking, dll load hook</PackageTags>
    <PackageProjectUrl>https://github.com/jcapellman/WET</PackageProjectUrl>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <RepositoryUrl>https://github.com/jcapellman/WET</RepositoryUrl>
    <Description>Windows Event Tracing wrapper .NET 5 library hooking File, Registry, TCP Connections, UDP Connetions, Processes and DLL Loading</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Diagnostics.Tracing.TraceEvent" Version="2.0.66" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
</Project>